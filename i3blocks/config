command=/usr/share/i3blocks/$BLOCK_NAME
separator_block_width=15
markup=none

[brightness]
command=echo "BRI" $(echo $(cat /sys/class/backlight/intel_backlight/actual_brightness)"*100/"$(cat /sys/class/backlight/intel_backlight/max_brightness) | bc)"%"
interval=once
signal=9

[volume]
label=VOL
instance=Master
interval=once
signal=10

[battery2]
markup=pango
interval=20

[time]
command=date '+%Y-%m-%d %H:%M:%S %p'
interval=1
