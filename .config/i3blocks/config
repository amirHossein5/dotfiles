command=/usr/share/i3blocks/$BLOCK_NAME
separator_block_width=15
markup=none

[bandwidth]
command=~/dotfiles/i3blocks/bandwidth -i ' ' -o ' '
interval=5

[brightness]
command=~/dotfiles/i3blocks/brightness
interval=once
signal=9

[volume]
label=VOL
instance=Master
interval=once
signal=10

[battery]
command=~/dotfiles/i3blocks/battery
interval=10

[time]
command=date '+%Y-%m-%d %H:%M:%S %p'
interval=1
